mode script

# Fibonacci sequence demo
writeln("AXIS Script - Fibonacci Demo")
writeln("============================")

# Count from 1 to 5
i: i32 = 1
while i <= 5:
    write("Count: ")
    writeln(i)
    i = i + 1

writeln("")

# Loop with break
j: i32 = 0
loop:
    j = j + 1
    when j > 3:
        break
    write("Loop iteration: ")
    writeln(j)

writeln("")
writeln("Script complete!")
