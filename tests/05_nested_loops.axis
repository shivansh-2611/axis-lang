// AXIS Example 05: Nested Loops
// verschachtelte while loops (quasi multiplikation)
// expected: 42 (6 * 7)

func main() -> i32:
    # 6 * 7 durch zÃ¤hlen simulieren
    outer: i32 = 0
    result: i32 = 0
    
    while outer < 6:
        inner: i32 = 0
        while inner < 7:
            result = result + 1
            inner = inner + 1
        outer = outer + 1
    
    give result  # 6 * 7 = 42
