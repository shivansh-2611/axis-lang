// AXIS Example 10: Infinite Loop mit Break
// repeat/loop fÃ¼r endlosschleifen
// expected: 50

func main() -> i32:
    counter: i32 = 0
    
    # endlosschleife - break zum rausspringen
    repeat:
        counter = counter + 1
        
        when counter == 50:
            break
    
    give counter  # 50
