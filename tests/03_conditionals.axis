// AXIS Example 03: Conditionals
// when/else branching zeug
// expected: 100

func main() -> i32:
    score: i32 = 85
    result: i32 = 0
    
    # conditional branching mit when/else
    when score >= 90:
        result = 100    # A
    else:
        when score >= 80:
            result = 100    # B (wir returnen 100 fÃ¼r test)
        else:
            when score >= 70:
                result = 70     # C
            else:
                result = 0      # fail
    
    give result
