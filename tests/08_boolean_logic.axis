// AXIS Example 08: Boolean Logic
// True/False und boolean negation
// expected: 42

func main() -> i32:
    # boolean variablen
    is_ready: bool = True
    is_blocked: bool = False
    
    # boolean negation
    can_proceed: bool = !is_blocked  # True
    
    when is_ready:
        when can_proceed:
            give 42  # passt
    
    give 0
