// AXIS Example 09: Integer Types
// verschiedene int größen: i8, i16, i32, i64
// expected: 42

func main() -> i32:
    # verschiedene integer sizes
    tiny: i8 = 42
    small: i16 = 1000
    normal: i32 = 100000
    large: i64 = 9000000000
    
    # unsigned types
    byte_val: u8 = 255
    word_val: u16 = 65535
    
    # tiny value checken
    result: i32 = 0
    
    when tiny == 42:
        when byte_val == 255:
            result = 42
    
    give result
